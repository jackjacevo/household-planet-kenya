{
  "name": "household-planet-kenya",
  "version": "1.0.0",
  "description": "E-commerce platform for household items in Kenya",
  "scripts": {
    "test:category-upload": "node test-category-upload.js",
    "dev:frontend": "cd household-planet-frontend && npm run dev",
    "dev:backend": "cd household-planet-backend && npm run start:dev",
    "build:frontend": "cd household-planet-frontend && npm run build",
    "build:backend": "cd household-planet-backend && npm run build",
    "start:frontend": "cd household-planet-frontend && npm start",
    "start:backend": "cd household-planet-backend && npm run start:prod"
  },
  "devDependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
}
